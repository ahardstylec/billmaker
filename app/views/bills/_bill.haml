%tr
	%td=link_to bill.billnr.to_s+"/"+Time.now().year.to_s, url(:bills, :show, id: bill.id)
	%td=bill.date.mon.to_s+"."+bill.date.year.to_s
	%td=bill.get_hours.round(2)
	%td=bill.bill.round(2)
	%td.last
		=button_to "pdf", url(:bills, :show, id: bill.id, format: :pdf), method: :get
		=button_to "edit", url(:bills, :edit, id: bill.id), method: :get
		=button_tag "delete", onclick: "deleteBill()", id: bill.id